<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="97dp" android:height="97dp" android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="#ff7ed321" />
            <solid android:color="#ffffffff" />
        </shape>
    </item>

    <item android:width="97dp" android:height="97dp" android:state_checked="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ffe2e7ec" />
            <solid android:color="#ffffffff" />
        </shape>
    </item>
</selector>